#!/usr/bin/env fish

set --export --global VG_PLUGIN_PATH $VG_PLUGIN_PATH "$VG_APP_DIR/tests/run_post_override/one" "$VG_APP_DIR/tests/run_post_override/two"

set o (vantage foo)

test "$o" = "post"

set s $status

if test $s -ne 0
    echo "    Thought plugin output should be post, it was $o"
end

exit $s
